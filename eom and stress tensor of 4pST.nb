Cell[TextData[{
 "The function ",
 StyleBox["measure",
  FontSlant->"Italic"],
 " is to give the square root of metric determinant, and ",
 StyleBox["tensorSim",
  FontSlant->"Italic"],
 " is useful for simplification."
}], "Text",
 CellChangeTimes->{{3.874313294839818*^9, 3.874313315182908*^9}, {
  3.874313856275031*^9, 
  3.8743138748249073`*^9}},ExpressionUUID->"d9d3b0b3-514e-4cd3-a761-\
b948ebcbbf9e"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"measure", "::", "usage"}], " ", "=", " ", "\"\<Sqrt[-g]\>\""}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"tensorSim", "::", "usage"}], " ", "=", " ", 
    "\"\<x//ContractMetric//Simplification\>\""}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"measure", "[", 
     RowBox[{"metric_", "?", "xAct`xTensor`MetricQ"}], "]"}], " ", ":=", "\n",
     "    ", 
    RowBox[{"Sqrt", "[", 
     RowBox[{
      RowBox[{"xAct`xTensor`SignDetOfMetric", "@", "metric"}], " ", 
      RowBox[{"ToExpression", "[", 
       RowBox[{"\"\<Det\>\"", "<>", 
        RowBox[{"ToString", "@", "metric"}], "<>", "\"\<[]\>\""}], "]"}]}], 
     "]"}]}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"tensorSim", "[", "expr_", "]"}], " ", ":=", "\n", "    ", 
   RowBox[{
    RowBox[{"expr", "//", "xAct`xTensor`ContractMetric"}], "//", 
    "xAct`xTensor`Simplification"}]}], ";"}]}], "Code",
 CellChangeTimes->{{3.868133279992435*^9, 3.868133279996047*^9}, {

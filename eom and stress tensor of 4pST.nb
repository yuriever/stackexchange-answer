 StyleBox["xAct`",
  FontSlant->"Italic"],
 " and ",
 StyleBox["GeneralUtilities`",
  FontSlant->"Italic"],
 ", hence ",
 StyleBox["Needs",
  FontSlant->"Italic"],
 " is better than ",
 StyleBox["Get",
  FontSlant->"Italic"],
